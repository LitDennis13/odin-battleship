{
    "extends" : ["airbnb"],
    "plugins" :[],
    "rules" : {
        "no-console" : "off",
        "no-unused-vars": "warn",
        "func-names": "off",
        "class-methods-use-this": "off",
        "indent": ["error", 4],
        "no-undef": "off",
        "prefer-regex-literals": "off",
        "no-prototype-builtins": "off",
        "no-plusplus": "off",
        "no-restricted-syntax": "off",
        "quotes": [2, "double", { "avoidEscape": true }]
    } 
}